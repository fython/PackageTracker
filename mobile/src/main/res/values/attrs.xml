<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="StatusBarHeaderView">
		<attr name="colorNormal" format="reference"/>
		<attr name="colorDark" format="reference"/>
		<attr name="enableMode" format="enum">
			<enum name="Kitkat" value="1"/>
			<enum name="Lollipop" value="2"/>
			<enum name="All" value="3"/>
		</attr>
	</declare-styleable>

	<declare-styleable name="ButtonBar">
		<attr name="leftText" format="string"/>
		<attr name="rightText" format="string"/>
		<attr name="leftEnabled" format="boolean"/>
		<attr name="rightEnabled" format="boolean"/>
	</declare-styleable>

	<attr name="bottomBarColor" format="reference"/>
	<attr name="bottomSheetColor" format="reference"/>
	<attr name="rootViewBackgroundColor" format="reference"/>

</resources>